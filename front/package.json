{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --public-host www.e-make.io --port $(ts-node -pe 'require(`../cross/config/local/common`).config.front.port')",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    
    "build:prod": "ng build --prod",
    "build:prod+docker": "npm run build:prod && npm run docker:build",
    "build:preprod": "ng build --configuration=preprod",
    "build:preprod+docker": "npm run build:preprod && npm run docker:build",
    "docker:build": "docker image build -t gerkin/e-make-front ."
  },
